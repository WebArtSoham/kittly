    <fieldset class="js product-form__input product-form__input--swatch {{ option.name | downcase }}-swatch">
        <legend class="form__label">{{ option.name }}:</legend>
        
        {%- for value in option.values -%}
        {%- liquid
            assign metaobject_color = value.swatch.color
            assign metaobject_image = value.swatch.image
            assign has_image = false
            assign background_color = ''
            assign background_image = ''

            if metaobject_image != blank
            assign swatch_image_url = metaobject_image | image_url: width: 100
            assign background_image = swatch_image_url
            assign has_image = true
            elsif metaobject_color != blank
            assign background_color = metaobject_color
            endif
            
            assign option_available = false
            assign option_index = option.position | minus: 1
            for variant in product.variants
            if variant.options[option_index] == value and variant.available
                assign option_available = true
                break
            endif
            endfor
        -%}

        <input type="checkbox"
            id="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
            name="{{ option.name }}"
            value="{{ value | escape }}"
            form="{{ product_form_id | default: 'product-form' }}"
            class="swatch-input"
            data-option-value="{{ value | escape }}"
            data-option-name="{{ option.name | escape }}"
            {% unless option_available %}disabled{% endunless %}
        />
        
        <label for="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
            class="swatch-label {% unless option_available %}soldout{% endunless %}"
            title="{{ value | escape }}"
            style="{% if has_image %}background-image: url({{ background_image }});{% else %}background-color: {{ background_color }};{% endif %} background-size: cover; background-position: center;"
